{
  "name": "airsim-js",
  "version": "0.3.1",
  "description": "TypeScript client library for Microsoft AirSim Simulator",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "keywords": [
    "airsim",
    "airsim simulator",
    "airsim client",
    "microsoft airsim",
    "robotics simulation",
    "typescript"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "build:docs": "typedoc -out ./docs/api --hideGenerator true --cleanOutputDir false ./src",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint ",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "author": "ros2jsguy <ros2jsguy@gmail.com> (https://github.com/ros2jsguy)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ros2jsguy/airsim-js.git"
  },
  "bugs": {
    "url": "https://github.com/ros2jsguy/airsim-js/issues"
  },
  "dependencies": {
    "@ros2jsguy/msgpack-rpc-node": "^0.2.0",
    "threejs-math": "^0.143.3",
    "tslib": "^2.4.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.29",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fps": "^0.0.3",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "rimraf": "~3.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "typedoc": "^0.23.2",
    "typescript": "^4.6.3"
  }
}
